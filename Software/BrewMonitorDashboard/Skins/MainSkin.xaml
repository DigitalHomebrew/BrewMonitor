    <Style x:Key="SmallWhiteLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="FontFamily" Value="Arial" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="#FFFA803D" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="DetailsLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="FontFamily" Value="Arial" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>
    <Style x:Key="LargeWhiteTextBlockStyle" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="FontSize" Value="24" />
        <Setter Property="FontFamily" Value="Arial" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="FontWeight" Value="Bold"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <ImageBrush x:Key="WindowBackgroundBrush" ImageSource="/BrewMonitorDashboard;component/Resources/gray_background.jpg" Stretch="UniformToFill"/>
    <Style x:Key="ChartStyle" TargetType="{x:Type sparrowToolkit:SparrowChart}">
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type sparrowToolkit:SparrowChart}">
                    <Border BorderBrush="{Binding BorderBrush, RelativeSource={RelativeSource TemplatedParent}}" BorderThickness="{Binding BorderThickness, RelativeSource={RelativeSource TemplatedParent}}" Background="{Binding Background, RelativeSource={RelativeSource TemplatedParent}}">
                        <sparrowToolkit:RootPanel x:Name="Part_rootDockPanel" LegendDock="{Binding Legend.Dock, RelativeSource={RelativeSource TemplatedParent}}">
                            <Grid x:Name="Part_OuterChartPanel"/>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Border x:Name="PART_containerBorder" Grid.Column="1" Grid.Row="1" Style="{Binding ContainerBorderStyle, RelativeSource={RelativeSource TemplatedParent}}">
                                    <Grid x:Name="Part_InnerChartPanel">
                                        <sparrowToolkit:ContainerCollection x:Name="PART_containers" IsBitmapInitialized="False" RenderingMode="{Binding RenderingMode, RelativeSource={RelativeSource TemplatedParent}}" Series="{Binding Series, RelativeSource={RelativeSource TemplatedParent}}" XAxis="{Binding XAxis, RelativeSource={RelativeSource TemplatedParent}}" YAxis="{Binding YAxis, RelativeSource={RelativeSource TemplatedParent}}"/>
                                    </Grid>
                                </Border>
                                <sparrowToolkit:AxesPanel x:Name="PART_TopAxes" Grid.Column="1" Grid.Row="0"/>
                                <sparrowToolkit:AxesPanel x:Name="PART_LeftAxes" Grid.Column="0" Grid.Row="1"/>
                                <sparrowToolkit:AxesPanel x:Name="PART_RightAxes" Grid.Column="2" Grid.Row="1"/>
                                <sparrowToolkit:AxesPanel x:Name="PART_BottomAxes" Grid.Column="1" Grid.Row="2"/>
                                <ContentPresenter x:Name="PART_yAxis" Content="{Binding YAxis, RelativeSource={RelativeSource TemplatedParent}}" Grid.Column="0" Grid.Row="1" Visibility="{Binding YAxis.Visibility, RelativeSource={RelativeSource TemplatedParent}}" Width="{Binding AxisWidth, RelativeSource={RelativeSource TemplatedParent}}">
                                    <ContentPresenter.Margin>
                                        <Binding ElementName="PART_containerBorder" Path="BorderThickness">
                                            <Binding.Converter>
                                                <sparrowToolkit:BorderThicknessToTopBottomConverter/>
                                            </Binding.Converter>
                                        </Binding>
                                    </ContentPresenter.Margin>
                                </ContentPresenter>
                                <ContentPresenter x:Name="PART_xAxis" Content="{Binding XAxis, RelativeSource={RelativeSource TemplatedParent}}" Grid.Column="1" Height="{Binding AxisHeight, RelativeSource={RelativeSource TemplatedParent}}" Grid.Row="2" Visibility="{Binding XAxis.Visibility, RelativeSource={RelativeSource TemplatedParent}}">
                                    <ContentPresenter.Margin>
                                        <Binding ElementName="PART_containerBorder" Path="BorderThickness">
                                            <Binding.Converter>
                                                <sparrowToolkit:BorderThicknessToLeftAndRightConverter/>
                                            </Binding.Converter>
                                        </Binding>
                                    </ContentPresenter.Margin>
                                </ContentPresenter>
                            </Grid>
                        </sparrowToolkit:RootPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>